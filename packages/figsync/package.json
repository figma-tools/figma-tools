    "jest-diff": "^28.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.7.9",
    "case-anything": "^2.1.10",
    "svg-to-jsx": "^1.0.4",
    "svgson": "^5.2.1",
    "tsup": "6.2.2",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
